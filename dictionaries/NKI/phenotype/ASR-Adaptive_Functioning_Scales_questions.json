    "ASRAF_43": {
        "LongName": "FRIENDS SCALE TOTAL RAW SCORE",
        "Description": "Calculation = [ASRAF_03]+[ASRAF_04]+[ASRAF_05]+[ASRAF_06]"
    },
    "ASRAF_44": {
        "LongName": "SPOUSE/PARTNER TOTAL RAW SCORE",
        "Description": "Calculation = iif ([ASRAF_08]==0,'NA',\r\n(([ASRAF_09]+[ASRAF_11]+[ASRAF_12]+[ASRAF_15])-([ASRAF_10]+[ASRAF_13]+[ASRAF_14]+[ASRAF_16])))\r\n\r\n\r\n"
    },
    "ASRAF_45": {
        "LongName": "MEAN SCORE FOR FAMILY SCALE",
        "Description": "Calculation = (iif ([ASRAF_17]==1,0, iif ([ASRAF_17]==2,1, iif ([ASRAF_17]==3,2)))+\r\niif([ASRAF_18]==1,0, iif ([ASRAF_18]==2,1, iif ([ASRAF_18]==3,2)))+\r\niif([ASRAF_19]==1,0, iif ([ASRAF_19]==2,1, iif ([ASRAF_19]==3,2)))+\r\niif([ASRAF_20]==1,0, iif ([ASRAF_20]==2,1, iif ([ASRAF_20]==3,2)))+\r\niif([ASRAF_22]==1,0, iif ([ASRAF_22]==2,1, iif ([ASRAF_22]==3,2)))+\r\niif([ASRAF_22a]==1,0, iif ([ASRAF_22a]==2,1, iif ([ASRAF_22a]==3,2)))+\r\niif([ASRAF_22b]==1,0, iif ([ASRAF_22b]==2,1, iif ([ASRAF_22b]==3,2)))+\r\niif([ASRAF_22c]==1,0, iif ([ASRAF_22c]==2,1, iif ([ASRAF_22c]==3,2)))+\r\niif([ASRAF_23]==1,0, iif ([ASRAF_23]==2,1, iif ([ASRAF_23]==3,2))))\r\n/\r\n (iif( [ASRAF_17] > 0, (iif( [ASRAF_17] < 4, 1, 0)), 0) + \r\n  iif( [ASRAF_18] > 0, (iif( [ASRAF_18] < 4, 1, 0)), 0) + \r\n iif( [ASRAF_19] > 0, (iif( [ASRAF_19] < 4, 1, 0)), 0) + \r\n iif( [ASRAF_20] > 0, (iif( [ASRAF_20] < 4, 1, 0)), 0) + \r\n iif( [ASRAF_22] > 0, (iif( [ASRAF_22] < 4, 1, 0)), 0) + \r\n iif( [ASRAF_22a] > 0, (iif( [ASRAF_22a] < 4, 1, 0)), 0) + \r\n iif( [ASRAF_22b] > 0, (iif( [ASRAF_22b] < 4, 1, 0)), 0) + \r\n iif( [ASRAF_22c] > 0, (iif( [ASRAF_22c] < 4, 1, 0)), 0) + \r\n iif( [ASRAF_23]> 0, (iif( [ASRAF_23] < 4, 1, 0)), 0))\r\n\r\n"
    },
    "ASRAF_46": {
        "LongName": "TOTAL RAW SCORE FOR THE JOB SCALE",
        "Description": "Calculation = iif ([ASRAF_24]==0,'NA', (([ASRAF_25]+[ASRAF_27])-([ASRAF_26]+[ASRAF_28]+[ASRAF_30]+[ASRAF_31]+[ASRAF_32]+[ASRAF_33])))\r\n\r\n\r\n"
    },
    "ASRAF_47": {
        "LongName": "TOTAL RAW SCORE FOR EDUCATION SCALE",
        "Description": "Calculation = iif([ASRAF_34]==0,'NA',(([ASRAF_35]+[ASRAF_36]+[ASRAF_38])-([ASRAF_37]+[ASRAF_39])))\r\n\r\n"
    },
